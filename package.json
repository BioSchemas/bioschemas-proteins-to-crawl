{
  "name": "bioschemas-proteins-to-crawl",
  "version": "0.0.1",
  "description": "A simple app displaying proteins with Bioschemas markup so a crawler can get the info",
  "scripts": {
    "build": "rimraf ./dist && mkdirp ./dist && rollup -c",
    "rollup-watch": "rollup -cw",
    "browser-sync": "browser-sync start --server -f dist/**/*.*",
    "serve": "npm run rollup-watch & npm run browser-sync",
    "npm-linkall": "npm link protvista-uniprot-entry-adapter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bioschemas/bioschemas-proteins-to-crawl.git"
  },
  "keywords": [
    "Bioschemas",
    "proteins",
    "crawler"
  ],
  "author": "ljgarcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bioschemas/bioschemas-proteins-to-crawl/issues"
  },
  "homepage": "https://github.com/bioschemas/bioschemas-proteins-to-crawl#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "4.3.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1"
  },
  "dependencies": {}
}
